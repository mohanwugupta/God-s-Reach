# Google Sheets Configuration
GOOGLE_SHEETS_CREDENTIALS_FILE=credentials.json
GOOGLE_SHEETS_SPREADSHEET_ID=your_spreadsheet_id
GOOGLE_SHEETS_WORKSHEET_NAME=Sheet1

# LLM Configuration
ANTHROPIC_API_KEY=your_anthropic_key_here
OPENAI_API_KEY=your_openai_key_here
LLM_PROVIDER=claude  # Options: claude, openai, qwen
LLM_MODEL=claude-3-sonnet-20240229
LLM_TEMPERATURE=0
LLM_MAX_TOKENS=4096
LLM_BUDGET_USD=10.00
LLM_ENABLE=false  # Set to true to enable LLM-assisted extraction

# Qwen (local model) Configuration
QWEN_MODEL_PATH=./models/qwen2.5
QWEN_ENABLE=false

# MATLAB Configuration
MATLAB_AVAILABLE=false
MATLAB_ENGINE_PATH=

# Database Configuration
DATABASE_PATH=./out/designspace.db

# Logging
LOG_LEVEL=INFO
LOG_FILE=./out/logs/extractor.log

# Extraction Settings
EXTRACTION_WORKERS=4
ENABLE_MULTIPROCESSING=true
CACHE_ENABLED=true
CACHE_DIR=./.cache

# Conflict Resolution
CONFLICT_POLICY=precedence  # Options: precedence, manual, ignore
